import java.util.*;
import java.io.*;

class Solution{
    public static void main(String []argh) {
        int sicaklik,cd;

        Scanner input = new Scanner(System.in);

        System.out.println("Sıcaklık giriniz: ");
        sicaklik = input.nextInt();
        if (sicaklik<5)
        {
            System.out.println("kayak yapmak için hava ideal");
        }
        if (sicaklik>=5 && sicaklik<15)
        {
            System.out.println("Sinema için hava ideal");
        }
        if (sicaklik>=15&&sicaklik<25)
        {
            System.out.println("Piknik için hava ideal");
        }
        if (sicaklik>25)
        {
            System.out.println("Yüzme için hava ideal");
        }

    }

}



